        "@angular/platform-browser-dynamic": "^8.0.0",
        "@angular/router": "^8.0.0",
        "@nestjs/common": "^6.0.0",
        "@nestjs/core": "^6.0.0",
        "@nestjs/platform-express": "^6.0.0",
        "@nestjs/typeorm": "^6.1.3",
        "@ngrx/effects": "^8.0.0",
        "@ngrx/entity": "^8.0.0",
        "@ngrx/router-store": "^8.0.0",
        "@ngrx/store": "^8.0.0",
        "@ptsecurity/cdk": "^8.0.0-beta.7",
        "@ptsecurity/mosaic": "^8.0.0-beta.7",
        "@ptsecurity/mosaic-icons": "2.10.0",
        "@ptsecurity/mosaic-moment-adapter": "^8.0.0-beta.7",
        "@types/faker": "^4.1.5",
        "faker": "^4.1.0",
        "messageformat": "^2.2.1",
        "moment": "^2.24.0",
        "rxjs": "~6.5.2",
        "sqlite": "^3.0.3",
        "tslib": "^1.9.0",
        "typeorm": "^0.2.18",
        "zone.js": "~0.9.1"
